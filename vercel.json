{
  "name": "asymmetrix-nextjs",
  "version": 2,
  "git": {
    "deploymentEnabled": {
      "main": true,
      "master": true,
      "develop": true,
      "staging": true
    }
  },
  "crons": [
    {
      "path": "/api/cron/warm-sectors",
      "schedule": "0 */2 * * *"
    }
